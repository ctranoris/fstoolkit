   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="runningScenariosSelected">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <test
                     property="gr.upatras.ece.nam.fstoolkit.propertyTestersNameSpace.isSelectedScenarios">
               </test></iterate>
         </with>
      </definition>
      <definition
            id="gr.upatras.ece.nam.fstoolkit.resourceRequestSelected">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <test
                     property="gr.upatras.ece.nam.fstoolkit.propertyTestersNameSpace.isSelectedResourceRequest">
               </test>
            </iterate>
         </with>
      </definition>
      <definition
            id="gr.upatras.ece.nam.fstoolkit.resourceRequestGroup">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
